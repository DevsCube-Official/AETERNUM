execute if score @s aeternum.energy.bar_type.base matches ..0 run function aeternum:ability_wheel/show_en/empty
execute if score @s aeternum.energy.bar_type.base matches 20.. run function aeternum:ability_wheel/show_en/full
execute if score @s aeternum.energy.bar_type.base matches 1..19 run function aeternum:ability_wheel/show_en/half